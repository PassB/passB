{
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "manifest_version": 2,
  "version": "0.1",
  "homepage_url": "https://github.com/phryneas/passb",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "applications": {
    "gecko": {
      "id": "passb@phryneas.de",
      "strict_min_version": "45.0"
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "built/background-script.js"
    ]
  },
  "permissions": ["nativeMessaging", "tabs", "activeTab"]
}

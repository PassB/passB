{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "interface-name": false,
    "quotemark": "single",
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "no-namespace": false,
    "ban-types": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "member-variable-declaration"
    ]
  },
  "rulesDirectory": []
}